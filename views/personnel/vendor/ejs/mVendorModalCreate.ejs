<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">&times;</button>
    <h4 class="modal-title">新增廠商</h4>
</div>
<div class="modal-body">
    <form id="vendorCreate" class="form-horizontal">
        <div class="row form-group">
            <label class="control-label col-lg-3 required" for='txtCreateVendorCompany'>廠商名稱</label>
            <div class="col-lg-8 has-feedback">
                <input id="txtCreateVendorCompany" name="txtCreateVendorCompany" type="text" placeholder="請輸入廠商名稱(公司)" class="form-control input-sm"
                    required="true" />
                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3 required" for="txtCreateVendorType">廠商類型</label>
            <div class="col-lg-8">
                <select id="txtCreateVendorType" name="txtCreateVendorType" class="form-control input-sm">
                <% for(var i in vendorType) {%>
                    <option value="<%= vendorType[i].id%>"><%= vendorType[i].type%></option>
                <% } %>
                </select>
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3 required" for='txtCreateVendorName'>連絡人姓名</label>
            <div class="col-lg-8 has-feedback">
                <input id="txtCreateVendorName" name="txtCreateVendorName" type="text" placeholder="請輸入連絡人姓名" class="form-control input-sm"
                    required="true" />
                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorTitle'>連絡人職稱</label>
            <div class="col-lg-8">
                <input id="txtCreateVendorTitle" name="txtCreateVendorTitle" type="text" placeholder="連絡人職稱" class="form-control input-sm"
                />
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorAgent'>代理人姓名</label>
            <div class="col-lg-8">
                <input id="txtCreateVendorAgent" name="txtCreateVendorAgent" type="text" placeholder="請輸入代理人姓名" class="form-control input-sm"
                />
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorPhone'>市內電話</label>
            <div class="col-lg-8">
                <input id="txtCreateVendorPhone" name="txtCreateVendorPhone" type="tel" placeholder="請輸入廠商市內電話" class="form-control input-sm"
                />
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorExtension'>分機</label>
            <div class="col-lg-8">
                <input id="txtCreateVendorExtension" name="txtCreateVendorExtension" type="tel" placeholder="請輸入廠商分機號碼" class="form-control input-sm"
                />
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3 required" for='txtCreateVendorCellphone'>手機號碼</label>
            <div class="col-lg-8 has-feedback">
                <input id="txtCreateVendorCellphone" name="txtCreateVendorCellphone" type="tel" placeholder="請輸入廠商/連絡人手機號碼" class="form-control input-sm"
                    required="true" />
                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorFax'>傳真號碼</label>
            <div class="col-lg-8">
                <input id="txtCreateVendorFax" name="txtCreateVendorFax" type="tel" placeholder="請輸入廠商傳真號碼" class="form-control input-sm"
                />
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorFacebook'>Facebook</label>
            <div class="col-lg-8">
                <input id="txtCreateVendorFacebook" name="txtCreateVendorFacebook" type="text" placeholder="請輸入廠商/連絡人Facebook" class="form-control input-sm"
                />
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorEmail'>E-Mail</label>
            <div class="col-lg-8">
                <input id="txtCreateVendorEmail" name="txtCreateVendorEmail" type="email" placeholder="請輸入廠商/連絡人E-mail" class="form-control input-sm"
                />
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorLine'>Line</label>
            <div class="col-lg-8">
                <input id="txtCreateVendorLine" name="txtCreateVendorLine" type="text" placeholder="請輸入廠商/連絡人Line" class="form-control input-sm"
                />
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorWechat'>WeChat</label>
            <div class="col-lg-8">
                <input id="txtCreateVendorWechat" name="txtCreateVendorWechat" type="text" placeholder="請輸入廠商/連絡人WeChat" class="form-control input-sm"
                />
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorAddr'>地址</label>
            <div class="col-lg-8">
                <input id="txtCreateVendorAddr" name="txtCreateVendorAddr" type="text" placeholder="請輸入廠商地址" class="form-control input-sm"
                />
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3 required" for='txtCreateVendorBankcode'>銀行代碼</label>
            <div class="col-lg-8 has-feedback">
                <input id="txtCreateVendorBankcode" name="txtCreateVendorBankcode" type="text" placeholder="請輸入廠商銀行代碼" class="form-control input-sm"
                    required="true" />
                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3 required" for='txtCreateVendorBankacct'>銀行帳戶</label>
            <div class="col-lg-8 has-feedback">
                <input id="txtCreateVendorBankacct" name="txtCreateVendorBankacct" type="text" placeholder="請輸入廠商銀行帳戶" class="form-control input-sm"
                    required="true" />
                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3 required" for='txtCreateVendorBankname'>銀行帳戶戶名</label>
            <div class="col-lg-8 has-feedback">
                <input id="txtCreateVendorBankname" name="txtCreateVendorBankname" type="text" placeholder="請輸入廠商銀行帳戶戶名" class="form-control input-sm"
                    required="true" />
                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for='txtCreateVendorIswork'>合作中</label>
            <div class="col-lg-8 checkbox">
                <label>
                    <input id="txtCreateVendorIswork" name="txtCreateVendorIswork" type="checkbox" value="1" />合作中</label>
            </div>
        </div>
        <div class="row form-group">
            <label class="control-label col-lg-3" for="txtCreateVendorRemark">備註</label>
            <div class="col-lg-8">
                <textarea id="txtCreateVendorRemark" name="txtCreateVendorRemark" class="form-control" rows="5"></textarea>
            </div>
        </div>
    </form>
</div>
<div class="modal-footer">
    <div class="col-lg-8">
        <div id="createMsgArea" class="msgArea" style="text-align: left;"></div>
    </div>
    <button id="createCheck" class="btn btn-info" type="submit" form="vendorCreate">送出</button>
    <button id="createCancel" class="btn btn-default" type="button" data-dismiss="modal">取消</button>
</div>